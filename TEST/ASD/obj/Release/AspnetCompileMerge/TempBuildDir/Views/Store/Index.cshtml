@model IEnumerable<ASD.Models.Product>
@{
    ViewBag.Title = "Store";
}
<h3>Which product would you like to purchase?</h3>

<p>
    Select from @Model.Count() products:
</p>
<ul>
    @foreach (var item in Model)
    {
        <li>@Html.ActionLink(item.Product_name, "Details", new { id = item.ProductID })</li>
    }
</ul>
